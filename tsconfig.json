{
    "compilerOptions": {
        "strict": true,
        "module": "esnext",
        "target": "esnext",
        "lib": [
            "esnext"
        ],
        "moduleResolution": "bundler",
        "noEmit": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*.ts", "src/**/*.tsx", "worker-configuration.d.ts"]
}